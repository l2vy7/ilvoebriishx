<?xml version="1.0" encoding="utf-8"?>
<com.appnext.nativeads.NativeAdView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/advin_native" android:layout_width="match_parent" android:layout_height="wrap_content">
    <FrameLayout android:id="@+id/fl_advin" android:layout_width="match_parent" android:layout_height="wrap_content">
        <androidx.cardview.widget.CardView android:layout_width="match_parent" android:layout_height="wrap_content" app:cardBackgroundColor="#fff" app:cardCornerRadius="4dp" cardUseCompatPadding="true" contentPadding="0dp">
            <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
                <com.appnext.nativeads.MediaView android:id="@+id/ad_img_main" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <LinearLayout android:orientation="horizontal" android:padding="5dp" android:layout_width="match_parent" android:layout_height="wrap_content">
                    <ImageView android:layout_gravity="center_vertical" android:id="@+id/ad_img" android:visibility="gone" android:layout_width="40dp" android:layout_height="40dp" android:layout_marginLeft="10dp" android:layout_marginRight="5dp" android:layout_marginStart="10dp" android:layout_marginEnd="5dp"/>
                    <LinearLayout android:layout_gravity="center_vertical" android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginRight="5dp">
                        <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                            <TextView android:textSize="14dp" android:textStyle="bold" android:textColor="#ff000000" android:layout_gravity="center_vertical" android:id="@+id/ad_tit" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="5dp" android:layout_marginEnd="5dp"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank1" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank1_medio" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_medio" android:rotationY="@integer/angle_rtl_180"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank1_g" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_g"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank2" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank2_medio" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_medio" android:rotationY="@integer/angle_rtl_180"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank2_g" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_g"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank3" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank3_medio" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_medio" android:rotationY="@integer/angle_rtl_180"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank3_g" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_g"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank4" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank4_medio" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_medio" android:rotationY="@integer/angle_rtl_180"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank4_g" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_g"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank5" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank5_medio" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_medio" android:rotationY="@integer/angle_rtl_180"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/rank5_g" android:visibility="gone" android:layout_width="14dp" android:layout_height="14dp" android:src="@drawable/favorito_p_2_g"/>
                            <ImageView android:layout_gravity="center_vertical" android:id="@+id/appnext_logo" android:layout_width="wrap_content" android:layout_height="14dp" android:layout_marginLeft="10dp" android:src="@drawable/info" android:adjustViewBounds="true" android:layout_marginStart="10dp"/>
                        </LinearLayout>
                        <TextView android:textSize="13dp" android:textColor="#ff000000" android:id="@+id/ad_descr" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="2"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </FrameLayout>
</com.appnext.nativeads.NativeAdView>
