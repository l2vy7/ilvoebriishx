<?xml version="1.0" encoding="utf-8"?>
<com.appnext.nativeads.NativeAdView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/advin_native" android:layout_width="match_parent" android:layout_height="wrap_content">
    <FrameLayout android:id="@+id/fl_advin" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="match_parent">
            <ImageView android:layout_gravity="center_vertical" android:id="@+id/ad_img" android:visibility="gone" android:layout_width="40dp" android:layout_height="40dp" android:layout_marginLeft="4dp" android:layout_marginStart="4dp"/>
            <TextView android:gravity="center_vertical" android:id="@+id/ad_tit" android:background="@drawable/fondo_rounded" android:padding="4dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="4dp" android:layout_marginRight="4dp" android:maxLines="2"/>
        </LinearLayout>
        <ProgressBar android:layout_gravity="center" android:id="@+id/ad_pb" android:visibility="gone" android:layout_width="40dp" android:layout_height="40dp" style="@style/pb_inverse"/>
    </FrameLayout>
</com.appnext.nativeads.NativeAdView>
