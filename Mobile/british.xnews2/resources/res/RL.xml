<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:id="@+id/privacy_icon_container" android:layout_width="17dp" android:layout_height="match_parent" android:layout_alignParentTop="true" android:layout_alignParentRight="true" android:layout_alignParentEnd="true">
        <ImageView android:id="@+id/privacy_icon" android:background="@drawable/apnxt_na_i_icon_dark" android:layout_width="12dp" android:layout_height="12dp" android:layout_marginTop="6dp" android:layout_marginRight="5dp" android:alpha="0.2" android:layout_marginEnd="5dp"/>
    </FrameLayout>
    <ImageView android:id="@+id/fake_privacy_icon" android:background="@drawable/apnxt_na_i_icon_dark" android:visibility="invisible" android:layout_width="17dp" android:layout_height="12dp" android:layout_marginTop="5dp" android:layout_alignParentLeft="true" android:layout_alignParentTop="true" android:layout_alignParentStart="true"/>
    <FrameLayout android:id="@+id/design_native_ads_container" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="4dp" android:layout_toLeftOf="@+id/privacy_icon_container" android:layout_toRightOf="@+id/fake_privacy_icon" android:layout_toStartOf="@+id/privacy_icon_container" android:layout_toEndOf="@+id/fake_privacy_icon"/>
</RelativeLayout>
